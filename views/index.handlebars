<div class="jumbotron" id="head">
  <h1>Todo Tournament!!!</h1>
  <h2>Leave of Champions Edition!!</h2>
  <h3>Third Strike!</h3>
</div>

<div class="container" id="third">
  <div class="row">
    <div class="col-8 offset-2">
      <h1>{{msg}}</h1>
      <h2></h2>
      <form class="clearfix mb-4" action="POST">
        <div class="form-group">
          <label for="example-text">Add tasks list to your Tournament</label>
          <input type="text" id="example-text" class="form-control" aria-describedby="example-text"
            placeholder="Enter Task Here">
        </div>
        <div class="form-group">
          <label for="example-description">Round 1!</label>
          {{!-- <textarea class="form-control" id="example-description" aria-describedby="example-description" --}}
            {{!-- placeholder="Enter A Reason Why This Is Important"></textarea> --}}
        </div>
        <button id="submit" class="btn btn-success float-right">Submit</button>
      </form>
      <h3>League of Champions!</h3>
      <ul id="example-list" class="list-group">
        {{#each todos}}
        <li data-id="{{this.id}}" class="list-group-item">
          <img src="{{this.character}}">
          <p>{{this.name}}</p>
          <button class="btn btn-danger float-right delete">ï½˜</button>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>